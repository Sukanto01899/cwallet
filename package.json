{
  "name": "cwallet",
  "version": "0.1.2",
  "private": false,
  "description": "A TypeScript CLI tool",
  "license": "MIT",
  "keywords": [
    "cli",
    "wallet",
    "crypto",
    "ethereum",
    "evm"
  ],
  "type": "module",
  "bin": {
    "cwallet": "dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsx src/cli.ts",
    "build": "tsup",
    "prepublishOnly": "npm run build",
    "lint": "eslint .",
    "format": "prettier -w .",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "start": "node dist/cli.js"
  },
  "dependencies": {
    "commander": "^12.1.0",
    "ethers": "^6.16.0",
    "inquirer": "^13.2.5",
    "keytar": "^7.9.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.1",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.4.2",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
